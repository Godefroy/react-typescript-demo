{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "pretty": true,
    "strictNullChecks": true
  }
}
